@model IEnumerable<PlayerDisplayResponse>
<div class="dropdown">
    <button class="btn btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
        Ülkeler
    </button>
    <ul class="dropdown-menu">
        <li><a asp-action="Index" asp-controller="Player" asp-route-id="" class="dropdown-item">Ülkeler</a></li>
        @foreach (var item in Model)
        {
            <li><a asp-action="Index" asp-controller="Player" asp-route-id="@item.Nationality" class="dropdown-item">@item.Nationality</a></li>
        }
    </ul>
</div>










